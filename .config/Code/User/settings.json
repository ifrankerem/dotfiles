  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": { "foreground": "#75715E", "fontStyle": "italic" }
      },
      {
        "scope": ["string", "constant.other.character", "punctuation.definition.string"],
        "settings": { "foreground": "#E6DB74" }
      },
      {
        "scope": ["constant.numeric", "constant.language", "constant.character.escape"],
        "settings": { "foreground": "#AE81FF" }
      },
      {
        "scope": ["keyword", "storage.type", "storage.modifier"],
        "settings": { "foreground": "#FD971F" }
      },
      {
        "scope": ["keyword.operator", "punctuation.separator", "meta.preprocessor", "entity.name.function.preprocessor"],
        "settings": { "foreground": "#F92672" }
      },
      {
        "scope": ["support.type", "entity.name.type", "entity.name.namespace", "entity.name.scope-resolution"],
        "settings": { "foreground": "#66D9EF" }
      },
      {
        "scope": ["entity.name.function", "support.function"],
        "settings": { "foreground": "#A6E22E" }
      },
      {
        "scope": ["variable", "meta.definition.variable"],
        "settings": { "foreground": "#F8F8F2" }
      },
      {
        "scope": ["entity.name.type.class", "entity.name.type.struct", "entity.name.type.enum"],
        "settings": { "foreground": "#66D9EF", "fontStyle": "bold" }
      },
      {
        "scope": ["string.quoted.double.include", "string.quoted.other.lt-gt.include"],
        "settings": { "foreground": "#E6DB74" }
      }
    ]
  },
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "type": "#66D9EF",
      "class": "#66D9EF",
      "enum": "#66D9EF",
